var Alloy=require("alloy"),_=Alloy._,Backbone=Alloy.Backbone;Alloy.Globals.getGeneralData=function(e){var e="1J0HGpursjzDKNKbjcutA8vY974D8u7dK32264aL3ARY",o="https://spreadsheets.google.com/feeds/list/"+e+"/od6/public/basic?hl=en_US&alt=json",n="gss"+e+".txt",l=Ti.Network.createHTTPClient({onload:function(e){try{json=JSON.parse(this.responseText),console.log("json data after download : "+JSON.stringify(json));var o=Ti.Filesystem.getFile(Ti.Filesystem.tempDirectory,n);if(o.exists()&&o.writeable){var l=o.deleteFile();Ti.API.info(1==l?"success":"fail")}o.write(this.responseText),Alloy.Collections.data.deleteAll();for(var t=0;+json.feed.entry.length>t;t++){var s=Alloy.createModel("data",{col1:json.feed.entry[t].title.$t,col2:json.feed.entry[t].content.$t,col3:"none",col4:"none",col5:"none",col6:"none",col7:"none",col8:"none",col9:"none",col10:"none",col11:"none",col12:"none",col13:"none",col14:"none",col15:"none"});s.save()}}catch(e){Ti.API.info("cathing e: "+e)}}});l.onerror=function(e){alert(e)},l.open("GET",o),l.send(),alert(" Data were successfuly downloaded from "+o+". Please proceed.")},Alloy.createController("index");